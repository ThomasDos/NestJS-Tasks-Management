generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Task {
  id          String        @id @default(uuid())
  title       String 
  description String 
  status      TaskStatus @default(OPEN)
  picture_url String 
}

enum TaskStatus {
  OPEN 
  IN_PROGRESS
  DONE
}